{
    "scripts": {
        "js:browserify": "browserify ./_js/tablewrap.js --outfile ./_public/tablewrap.js --debug --transform babelify",
        "js:uglify": "uglifyjs --compress --mangle --output ./_public/tablewrap.js ./_public/tablewrap.js",
        "js:demo:browserify": "browserify ./_js/demo.js --outfile ./_public/demo.js --debug --transform babelify",
        "js": "npm-run-all --sequential js:browserify js:uglify js:demo:browserify",
        "js:dev": "npm-run-all --sequential js:browserify js:demo:browserify",
        "html": "html-minifier --collapse-whitespace --output ./_public/index.html ./_html/index.html",
        "html:dev": "html-minifier --output ./_public/index.html ./_html/index.html",
        "browsersync": "from-env cross-var browser-sync start --reload-debounce 3000 --proxy %DOMAIN --files \"./_public/*.js\"",
        "watch:js": "onchange ./_js/*.js ./_js/**/*.js --initial --wait --delay 500 --await-write-finish -- onerror \"npm run js:dev --silent\" --title \"watch:js\" --message \"build failed\" --sound mute",
        "watch:html": "onchange ./_html/*.html ./_html/**/*.html --initial --wait --delay 500 --await-write-finish -- onerror \"npm run html:dev --silent\" --title \"watch:html\" --message \"build failed\" --sound mute",
        "build": "npm-run-all --parallel js html",
        "dev": "npm-run-all --parallel watch:* browsersync"
    },
